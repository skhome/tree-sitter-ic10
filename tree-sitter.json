{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ic10",
      "camelcase": "IC10",
      "title": "Stationeers IC10",
      "scope": "source.ic10",
      "path": ".",
      "file-types": [
        "ic10"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "injection-regex": "^ic10$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Stationeers IC10 grammar for tree-sitter",
    "authors": [
      {
        "name": "Sascha Krueger",
        "email": "skhome@users.noreply.github.com"
      }
    ],
    "links": {
      "repository": "https://github.com/skhome/tree-sitter-ic10"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
